{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "System": "Warning",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "HostsFilePath": "/usr/local/etc/hosts",
  "CnameFilePath": "/etc/dnsmasq.d/cname",
  "ReloadCommand": [ "pkill", "-HUP", "dnsmasq" ]
}
